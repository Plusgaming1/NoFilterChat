print("Hi")
local TABLE = {
  RemoteBans = {
    [1] = {
      Name = "Dominique011415";
      Reason = "Insulting Riley";
    }
    [2] = {
      Name = "5x9x7x2x7x9";
      Reason = "Test";
    }
  }
}
local banned = false
for i, plr2 in pairs(TABLE.RemoteBans) do
  if plr2.Name == _G.Player.Name then
    banned = true
    _G.Player:Kick("Reason: ".. plr2.Reason)
  end
end
if banned = false then
  print("Not banned")
else
  print("BANNED")
end
